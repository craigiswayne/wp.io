{
  "name": "craigiswayne/wp.io",
  "description": "My WordPress Playground",
  "license": "MIT",
  "authors": [
    {
      "name": "Craig Wayne",
      "email": "craigiswayne@gmail.com"
    }
  ],
  "minimum-stability": "beta",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": "^7.0",
    "composer/installers": "~1.0",
    "craigiswayne/wp-shanela": "^0.0.5",
    "johnpbloch/wordpress": "^4.9",
    "wpackagist-plugin/jetpack": "^5.4",
    "wpackagist-plugin/kirki": "^3",
    "wpackagist-plugin/timber-library": "^1.3",
    "wpackagist-plugin/wordpress-importer": "dev-trunk",
    "wpackagist-theme/hestia": "^1.1.50",
    "wpackagist-theme/twentyseventeen": "^1.3"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/tf-core/addons/tf-buyers_guide": [
        "24dotcom/tf-buyers-guide-addon"
      ],
      "wp-content/languages": [
        "24dotcom/tf-languages"
      ],
      "wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ],
      "wp-content/plugins/tf-core/addons/tf-native": [
        "24dotcom/tf-native-addon"
      ],
      "wp-content/plugins/tf-core/addons/tf-campaigns": [
        "24dotcom/tf-campaigns-addon"
      ],
      "wp-content/plugins/tf-core/addons/tf-popular_posts": [
        "24dotcom/tf-popular-posts-addon"
      ]
    }
  }
}
